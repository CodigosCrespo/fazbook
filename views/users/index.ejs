<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <a href="users/new">Add new user</a>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>Welcome to fazbook</h1>
    <% users.forEach(function(user) { %>
      <div>
        <span><%= user.email %></span>
        <span><%= user.firstname %></span>
        <span><%= user.lastname %></span>
        <span><%= user.dob %></span>
      </div>
    <% }); %>
  </body>
</html>

